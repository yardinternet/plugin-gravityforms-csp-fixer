{
	"name": "plugin/yard-gravityforms-csp-fixer",
	"description": "Yard | CSP Fixer",
	"authors": [
		{
			"name": "Yard | Digital Agency",
			"email": "info@yard.nl",
			"homepage": "https://www.yard.nl"
		}
	],
	"type": "wordpress-plugin",
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		}
	],
	"require": {
		"php": "^7.4|^8.0"
	},
	"require-dev": {
		"10up/wp_mock": "~0.5",
		"friendsofphp/php-cs-fixer": "^2.0",
		"mockery/mockery": "^1.5",
		"phpunit/phpunit": "~9.0",
		"szepeviktor/phpstan-wordpress": "^1.1"
	},
	"autoload": {
		"psr-4": {
			"Yard\\CSP\\": "./src/CSP"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Yard\\CSP\\Tests\\Unit\\": "./tests/Unit"
		}
	},
	"scripts": {
		"test": "clear && ./vendor/bin/phpunit  --testsuite 'Unit Test Suite' --colors=always",
		"format": "vendor/bin/php-cs-fixer fix",
		"phpstan": "./vendor/bin/phpstan analyse"
	}
}